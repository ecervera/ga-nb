    <UsarQi TimeStep="35" />

    <!-- NaoQi settings. Must match a name from %AL_DIR%/preferences/simulator.xml.
         It also controls the port used for NaoQi. -->
    <NaoQi Name="NaoSim1" InitialStiffness="1.0" />
    
    <!-- UsarSim settings -->
    <UsarSim IP="localhost" Port="3000" /> <!-- UsarSim settings -->
    <!--<UsarSimInit Class="USARBot.NaoRed" Location="-3.5,7.5,-0.5" Rotation="0,0,0" />--> 
	<UsarSimInit Class="USARBot.NaoV32Red" Location="-13.5,7.5,-0.35" Rotation="0,0,0" /> 
	
    <!-- UPIS -->
    <UPIS IP="localhost" Port="5003" Disable="0" />
</UsarNaoQi>